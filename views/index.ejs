<body>
    <div class="container">
        <h1>Ingrese producto</h1>
        <div class="row">
            <form>
                <div class="mb-3">
                   <label for="name" class="form-label">Nombre</label>
                   <input id="name" type="text" name="name">
                </div>
                <div class="mb-3">
                    <label for="name" class="form-label">Precio</label>
                    <input id="price" type="number" name="precio">
                 </div>
                 <div class="mb-3">
                    <label for="name" class="form-label">Foto URL</label>
                    <input id="photo" type="url" name="foto">
                 </div>
                <button id="btnForm" type="submit">ENVIAR</button>  
            </form>
            <br>
            <div class="my-3" id="tableProducts"></div>
            <br>
            <div id="chat"></div>
            <br>
            <div>
                <h3 id="username"></h3>
                <form>
                    <input id="msn" name="msn">
                    <button id="btnChat">Enviar</button>
                </form>
            </div>
        </div>
    </div>
    <br />
    
    
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
